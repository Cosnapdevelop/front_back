# Cosnap AI 前端环境变量配置文件
# 复制此文件为 .env 用于本地开发

# ================================
# 必需配置
# ================================

# 后端API地址 - 生产环境请替换为你的Render后端域名
VITE_API_BASE_URL=https://你的render后端域名.onrender.com

# ================================
# 可选配置
# ================================

# PWA推送通知密钥（可选，如不需要推送通知可不配置）
VITE_VAPID_PUBLIC_KEY=BIjbMpazpOqhKKaDqcc7RudDlyxTP5Q9x933yEgDkmPFKfNBbdIKtN-XdjSj2PY7HoYTArAtPMDC9YlnAXd290M

# 应用版本信息
VITE_APP_VERSION=1.0.0
VITE_BUILD_TIME=2024-08-20T10:00:00.000Z

# API性能配置
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# 功能开关
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_OFFLINE_MODE=false

# 缓存配置
VITE_ENABLE_API_CACHING=true
VITE_STATIC_CACHE_DURATION=86400000
VITE_API_CACHE_DURATION=300000

# ================================
# 监控和分析（可选）
# ================================

# Sentry错误监控
# VITE_SENTRY_DSN=你的Sentry DSN

# Google Analytics
# VITE_GA_ID=你的Google Analytics ID

# 百度统计
# VITE_BAIDU_ANALYTICS_ID=你的百度统计ID

# ================================
# 功能特性开关（可选）
# ================================

# VITE_FEATURE_NEW_UPLOADER=true
# VITE_FEATURE_ENHANCED_EFFECTS=true
# VITE_FEATURE_SOCIAL_SHARING=true
# VITE_FEATURE_ADVANCED_FILTERS=false
# VITE_FEATURE_REALTIME_PREVIEW=false
# VITE_ENABLE_BETA_FEATURES=false